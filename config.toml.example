[http]
#APIEndpointURL
api-address=""

#FrontEndAddress
frontend-address=""

# List of RTMP apps
applications = ["stream"]

# Path prefix to allow frontend to run on a subpath
#prefix = ""

# Allow CSRF cookie to be sent across http-connection, not recommended for production
#insecure = false

[store]
# Set store backend (file|consul)
#backend = "file"

[store.file]
# Configure file storage path relative to working directory
#path = "store.db"

[keycloak]
#Keycloak OAuth Client ID
kc-oauth-cl-id = ""

#Keycloak OAuth Client Secret
kc-oauth-cl-secret = ""

#Keycloak URL
keycloakurl = ""

#Keycloak Realm
keycloakrealm = ""